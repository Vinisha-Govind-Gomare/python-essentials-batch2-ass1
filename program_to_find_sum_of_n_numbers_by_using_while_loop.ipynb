        "number=int(input(\"Enter the number:\"))\r\n",
        "\r\n",
        "total= 0\r\n",
        "value= 1\r\n",
        "while(value<=number):\r\n",
        "    total=total+value\r\n",
        "    value=value+1\r\n",
        "    \r\n",
        "print(\"The Sum of natural numbers from 1 to {0}= {1}\".format(number,total))    \r\n",
        "    \r\n",
        "    "
